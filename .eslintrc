{
  "root": true,
  "extends": ["@nuxt/eslint-config", "plugin:prettier/recommended"],
  "rules": {
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "argsIgnorePattern": "^_",
        "ignoreRestSiblings": true,
        "destructuredArrayIgnorePattern": "^_"
      }
    ]
  },
  "overrides": [
    {
      "files": ["components/ui/**/*.vue"],
      "rules": {
        "vue/multi-word-component-names": 0
      }
    }
  ]
}
